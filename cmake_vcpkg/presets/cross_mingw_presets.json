{
    "version": 6,
    "configurePresets": [
        {
            "name": "mingw-x64-base(cross)",
            "hidden": true,
            "generator": "Unix Makefiles",
            "binaryDir": "${sourceDir}/build/${presetName}",
            "installDir": "${sourceDir}/build/${presetName}",
            "cacheVariables": {
                "CMAKE_C_COMPILER": "x86_64-w64-mingw32-gcc",
                "CMAKE_CXX_COMPILER": "x86_64-w64-mingw32-g++",
                "CMAKE_TOOLCHAIN_FILE": "$env{VCPKG_ROOT}/scripts/buildsystems/vcpkg.cmake",
                {% if build_type == "lib" and library_type == "SHARED" -%}
                "CMAKE_WINDOWS_EXPORT_ALL_SYMBOLS": "ON",
                {% endif -%}
                "CMAKE_EXPORT_COMPILE_COMMANDS": "ON"
            },
            "condition": {
                "type": "equals",
                "lhs": "${hostSystemName}",
                "rhs": "Linux"
            }
        },
        {
            "name": "mingw-x64-static-dbg(cross)",
            "displayName": "MinGW x64 Static Debug",
            "inherits": "mingw-x64-base(cross)",
            "cacheVariables": {
                "VCPKG_TARGET_TRIPLET": "x64-mingw-static",
                "CMAKE_BUILD_TYPE": "Debug"
            }
        },
        {
            "name": "mingw-x64-static-rel(cross)",
            "displayName": "MinGW x64 Static Release",
            "inherits": "mingw-x64-static-dbg(cross)",
            "cacheVariables": {
                "CMAKE_BUILD_TYPE": "Release"
            }
        },
        {
            "name": "mingw-x64-dynamic-dbg(cross)",
            "displayName": "MinGW x64 Dynamic Debug",
            "inherits": "mingw-x64-base(cross)",
            "cacheVariables": {
                "VCPKG_TARGET_TRIPLET": "x64-mingw-dynamic",
                "CMAKE_BUILD_TYPE": "Debug"
            }
        },
        {
            "name": "mingw-x64-dynamic-rel(cross)",
            "displayName": "MinGW x64 Dynamic Release",
            "inherits": "mingw-x64-dynamic-dbg(cross)",
            "cacheVariables": {
                "CMAKE_BUILD_TYPE": "Release"
            }
        }
    ],
    "buildPresets": [
        {
            "name": "mingw-x64-static-dbg-build(cross)",
            "displayName": "Build MinGW x64 Static Debug",
            "configurePreset": "mingw-x64-static-dbg(cross)",
            "description": "Build MinGW x64 Static Debug configuration"
        },
        {
            "name": "mingw-x64-static-rel-build(cross)",
            "displayName": "Build MinGW x64 Static Release",
            "configurePreset": "mingw-x64-static-rel(cross)",
            "description": "Build MinGW x64 Static Release configuration"
        },
        {
            "name": "mingw-x64-dynamic-dbg-build(cross)",
            "displayName": "Build MinGW x64 Dynamic Debug",
            "configurePreset": "mingw-x64-dynamic-dbg(cross)",
            "description": "Build MinGW x64 Dynamic Debug configuration"
        },
        {
            "name": "mingw-x64-dynamic-rel-build(cross)",
            "displayName": "Build MinGW x64 Dynamic Release",
            "configurePreset": "mingw-x64-dynamic-rel(cross)",
            "description": "Build MinGW x64 Dynamic Release configuration"
        }
    ],
    "testPresets": [],
    "packagePresets": [
        {
            "name": "mingw-x64-static-dbg-package(cross)",
            "displayName": "Package MinGW x64 Static Debug",
            "configurePreset": "mingw-x64-static-dbg(cross)",
            "description": "Create package for MinGW x64 Static Debug configuration"
        },
        {
            "name": "mingw-x64-static-rel-package(cross)",
            "displayName": "Package MinGW x64 Static Release",
            "configurePreset": "mingw-x64-static-rel(cross)",
            "description": "Create package for MinGW x64 Static Release configuration"
        },
        {
            "name": "mingw-x64-dynamic-dbg-package(cross)",
            "displayName": "Package MinGW x64 Dynamic Debug",
            "configurePreset": "mingw-x64-dynamic-dbg(cross)",
            "description": "Create package for MinGW x64 Dynamic Debug configuration"
        },
        {
            "name": "mingw-x64-dynamic-rel-package(cross)",
            "displayName": "Package MinGW x64 Dynamic Release",
            "configurePreset": "mingw-x64-dynamic-rel(cross)",
            "description": "Create package for MinGW x64 Dynamic Release configuration"
        }
    ],
    "workflowPresets": [
        {
            "name": "mingw-x64-static-dbg-workflow(cross)",
            "displayName": "Workflow MinGW x64 Static Debug",
            "steps": [
                {
                    "type": "configure",
                    "name": "mingw-x64-static-dbg(cross)"
                },
                {
                    "type": "build",
                    "name": "mingw-x64-static-dbg-build(cross)"
                },
                {
                    "type": "package",
                    "name": "mingw-x64-static-dbg-package(cross)"
                }
            ]
        },
        {
            "name": "mingw-x64-static-rel-workflow(cross)",
            "displayName": "Workflow MinGW x64 Static Release",
            "steps": [
                {
                    "type": "configure",
                    "name": "mingw-x64-static-rel(cross)"
                },
                {
                    "type": "build",
                    "name": "mingw-x64-static-rel-build(cross)"
                },
                {
                    "type": "package",
                    "name": "mingw-x64-static-rel-package(cross)"
                }
            ]
        },
        {
            "name": "mingw-x64-dynamic-dbg-workflow(cross)",
            "displayName": "Workflow MinGW x64 Dynamic Debug",
            "steps": [
                {
                    "type": "configure",
                    "name": "mingw-x64-dynamic-dbg(cross)"
                },
                {
                    "type": "build",
                    "name": "mingw-x64-dynamic-dbg-build(cross)"
                },
                {
                    "type": "package",
                    "name": "mingw-x64-dynamic-dbg-package(cross)"
                }
            ]
        },
        {
            "name": "mingw-x64-dynamic-rel-workflow(cross)",
            "displayName": "Workflow MinGW x64 Dynamic Release",
            "steps": [
                {
                    "type": "configure",
                    "name": "mingw-x64-dynamic-rel(cross)"
                },
                {
                    "type": "build",
                    "name": "mingw-x64-dynamic-rel-build(cross)"
                },
                {
                    "type": "package",
                    "name": "mingw-x64-dynamic-rel-package(cross)"
                }
            ]
        }
    ]
}